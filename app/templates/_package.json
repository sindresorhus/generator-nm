{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "My <%= superb %> module",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo <% if (babel) { %>src/*.js test*.js && npm run babel <% } %>&& <% if (nyc) { %>nyc <% } %>ava"<% if (babel) { %>,
    "babel": "babel src --out-dir=. --presets=es2015",
    "prepublish": "npm run babel"<% } %>
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0"
  <% } %>},
  "devDependencies": {
    "ava": "^0.14.0",<% if (babel) { %>
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",<% } %><% if (nyc) { %>
    "nyc": "^6.4.0",<% } %>
    "xo": "^0.14.0"
  }
}
